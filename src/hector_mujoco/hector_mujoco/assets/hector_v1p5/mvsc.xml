		<motor name="r_hip_pitch" joint="r_hip_pitch_joint" forcerange="-33.5 33.5" />
		<motor name="r_knee" joint="r_knee_joint" forcerange="-67.0 67.0"   />
		<motor name="r_ankle" joint="r_ankle_joint" forcerange="-33.5 33.5" />
	</actuator>	


